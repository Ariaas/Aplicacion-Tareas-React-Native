{
  "expo": {
    "name": "App de Tareas",
    "slug": "todo-app-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#6200ee",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appelectiva.tareas"
    },
    "android": {
      "package": "com.appelectiva.tareas",
      "adaptiveIcon": {
        "backgroundColor": "#6200ee"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
}
